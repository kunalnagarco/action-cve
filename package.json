{
  "name": "@kunalnagarco/action-cve",
  "version": "0.0.0",
  "private": true,
  "description": "Send GitHub vulnerability alerts to multiple platforms",
  "keywords": [
    "actions",
    "node",
    "setup",
    "cve",
    "vulnerabilities",
    "mitre",
    "github advisory database"
  ],
  "bugs": {
    "url": "https://github.com/kunalnagarco/action-cve/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kunalnagarco/action-cve.git"
  },
  "license": "MIT",
  "author": "Kunal Nagar <2741371+kunalnagar@users.noreply.github.com>",
  "main": "lib/main.js",
  "scripts": {
    "pre-commit": "lint-staged",
    "build": "tsc",
    "format": "prettier --write src/**/*.ts",
    "format-check": "prettier --check src/**/*.ts",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "yarn build && yarn format && yarn lint && yarn package && yarn test",
    "release": "semantic-release"
  },
  "lint-staged": {
    "package.json": [
      "sort-package-json"
    ],
    "src/**/*.{ts}": [
      "yarn lint",
      "yarn format",
      "yarn format-check"
    ]
  },
  "dependencies": {
    "@actions/core": "1.10.1",
    "@actions/github": "6.0.1",
    "@octokit/rest": "^20.0.2",
    "@octokit/types": "^13.5.0",
    "@pagerduty/pdjs": "2.2.4",
    "@slack/types": "2.12.0",
    "@slack/webhook": "7.0.6",
    "adaptivecards": "3.0.5",
    "nodemailer": "7.0.11"
  },
  "devDependencies": {
    "@kunalnagarco/eslint-config": "2.2.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/commit-analyzer": "13.0.1",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "10.3.5",
    "@semantic-release/release-notes-generator": "14.0.3",
    "@types/jest": "29.5.14",
    "@types/node": "20.19.27",
    "@types/nodemailer": "6.4.21",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vercel/ncc": "0.38.3",
    "conventional-changelog-conventionalcommits": "7.0.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.8.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.35.0",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "lint-staged": "15.5.2",
    "prettier": "3.7.3",
    "semantic-release": "23.1.1",
    "sort-package-json": "2.10.0",
    "swiper": "^11.1.9",
    "ts-jest": "29.2.4",
    "typescript": "5.9.3"
  },
  "packageManager": "yarn@4.1.1"
}
